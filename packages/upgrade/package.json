{
	"name": "@clerk/upgrade",
	"version": "0.0.0",
	"license": "MIT",
	"bin": "dist/cli.js",
	"type": "module",
	"engines": {
		"node": ">=16"
	},
	"scripts": {
		"build": "babel --out-dir=dist source --copy-files",
		"dev": "babel --out-dir=dist --watch source --copy-files",
